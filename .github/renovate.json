{
  "extends": ["config:js-app", ":semanticCommits"],
  "branchConcurrentLimit": 20,
  "packageRules": [
    {
      "matchPackagePrefixes": ["effect", "@effect/"],
      "groupName": "effect"
    },
    {
      "matchPackagePatterns": ["@nx-tools/*"],
      "groupName": "nx-tools"
    },
    {
      "matchPackageNames": ["node.js", "node"],
      "groupName": "node.js"
    }
  ]
}
